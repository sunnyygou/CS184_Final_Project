{
  "stages": {
    "color": {
      "shader": "final_tonemap.glsl",
      "output": [ "framebuffer" ],
      "textures": [ "output0" ],
      "parameters": [
        {
          "name": "colorFilter",
          "type": "vec3",
          "min": [ 0.0, 0.0, 0.0 ],
          "max": [ 1.0, 1.0, 1.0 ],
          "default": [ 1.0, 1.0, 1.0 ]
        }
      ]
    },
    "size": {
      "shader": "game_of_life.glsl",
      "output": [ "output0" ],
      "textures": [ "previous_output0" ],
      "parameters": [
        {
          "name": "sourceSize",
          "type": "float",
          "min": 10.0,
          "max": 30.0,
          "default": 15.0
        },
        {
          "name": "buoyancy",
          "type": "float",
          "min": -2.5,
          "max": 2.5,
          "default": 0.0
        }
      ]
    }
  }
}
